        <profile id="4b72-2bad-754b-2249" name="1. Fiery Form" publicationId="ca571888--pubN106502" page="186" hidden="false" typeId="ae70-4738-0161-bec0" typeName="灵能">
          <characteristics>
            <characteristic name="次元充能" typeId="5ffd-b800-c317-532a">1</characteristic>
