    <profile id="06710a9d-a2a8-638f-91b0-2af2fb3e95c2" name="装甲座舱" publicationId="ca571888--pubN67227" page="88" hidden="false" typeId="57617267656172204974656d23232344415441232323" typeName="装备">
      <characteristics>
        <characteristic name="描述" typeId="4465736372697074696f6e23232344415441232323">投掷D6, 4+免疫乘员震荡或震晕</characteristic>
