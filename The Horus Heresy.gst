    <selectionEntry id="63b0-0b66-5eb0-382a" name="装甲容器(遗产)" publicationId="ca571888--pubN73893" page="98" hidden="false" collective="false" import="true" type="upgrade">
      <selectionEntries>
        <selectionEntry id="91db-0420-d420-0f55" name="Munitorum Armoured Container Cache" hidden="false" collective="false" import="true" type="upgrade">
          <constraints>
