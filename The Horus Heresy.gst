          </characteristics>
        </profile>
        <profile id="2352-5409-9866-1b07" name="阿德克斯近防双联激光炮塔" publicationId="ca571888--pubN68042" page="264" hidden="false" typeId="576561706f6e23232344415441232323" typeName="武器">
