            <characteristic name="组成结构" typeId="d34f-9153-d292-adab">1 被摧毁的帝国登陆平台.</characteristic>
            <characteristic name="类型" typeId="98d6-2143-3017-6d46">Difficult terrain</characteristic>
            <characteristic name="出入口和射击口" typeId="beac-711d-f76d-e254">无</characteristic>
