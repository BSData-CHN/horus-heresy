        <profile id="999c-a5c3-6eb7-f139" name="3. Spontaneous Combustion" publicationId="ca571888--pubN106502" page="186" hidden="false" typeId="ae70-4738-0161-bec0" typeName="灵能">
          <characteristics>
            <characteristic name="次元充能" typeId="5ffd-b800-c317-532a">1</characteristic>
